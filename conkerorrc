// -*- Mode: js2; -*-
// Conkereor config file

editor_shell_command = "emacsclient -c";

title_format_fn = function (window) {
    var buf = window.buffers.current;
    var prefix = "conkeror : ";
    if (buf instanceof download_buffer) {
        prefix = "*download* ";
    } else if (buf instanceof special_buffer) {
        prefix = "*special* ";
    }
    return prefix + buf.description;
};

add_delicious_webjumps("jrockway");
define_webjump("cpan", "http://search.cpan.org/search?query=%s&mode=all");
